package  {		import flash.display.MovieClip;	import flash.display.Sprite;			public class graphics1 extends MovieClip {						public function graphics1() {			for(var i=50; i < stage.stageWidth - 50; i+= 50) {				for(var j=50; j < stage.stageHeight - 50; j+= 50) {					var ball = createBall((i+j)/20);					ball.x = i;					ball.y = j;					addChild(ball);				}			}					}				public function createBall(r) {			var s:Sprite = new Sprite();			s.graphics.beginFill(0xFFFF00);			s.graphics.drawCircle(0,0,r);			s.graphics.endFill();			s.graphics.lineStyle(1,0X000000,1.0);			s.graphics.moveTo(-r,-r);			s.graphics.lineTo(r,-r);			s.graphics.lineTo(r,r);			s.graphics.lineTo(-r,r);			s.graphics.lineTo(-r,-r);									return s;		}			}	}