package  {		import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.net.URLLoader;	import flash.net.URLVariables;	import flash.net.URLRequestMethod;	import flash.display.Loader;	import flash.events.Event;			public class simpleServerCom extends MovieClip {						var resXML : XML;		var myrequest : URLRequest;		var myloader : URLLoader;		var myvariables : URLVariables;				public function simpleServerCom() {						myrequest = new URLRequest('http://localhost/SIR1213/SIMPLE_FLASH/add.php');			myrequest.method = URLRequestMethod.GET;						myvariables = new URLVariables();			myvariables.n1 = 12;			myvariables.n2 = 33332;			// constructor code			myrequest.data = myvariables;						myloader = new URLLoader(myrequest);			myloader.addEventListener(Event.COMPLETE,ondata);					}				public function ondata(e:Event) {			var loader = e.target as URLLoader;			resXML = new XML(loader.data);			trace(resXML..valor.(@op1 == '12').text());		}							}	}