package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;			public class CFrasco extends MovieClip {						public function CFrasco() {			// constructor code			addEventListener(MouseEvent.MOUSE_DOWN,ondrag);		}				private function ondrag(e:MouseEvent) {			parent.setChildIndex(this,parent.numChildren-1);			startDrag();			//trace("t",e.target);			//trace("ct",e.currentTarget);			removeEventListener(MouseEvent.MOUSE_DOWN,ondrag);			addEventListener(MouseEvent.MOUSE_UP,offdrag);		}				private function offdrag(e:MouseEvent) {			stopDrag();			addEventListener(MouseEvent.MOUSE_DOWN,ondrag);			removeEventListener(MouseEvent.MOUSE_UP,offdrag);		}									}	}