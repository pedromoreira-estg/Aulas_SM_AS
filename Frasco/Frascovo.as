package  {		import flash.display.MovieClip;			public class Frascovo extends MovieClip {				var frascosMC = new MovieClip();				var frascos : Array = new Array();				var nfrascos = 3;		//var gemada : Array;		var ngemas : uint = 5;						public function Frascovo() {			// constructor code			addChild(frascosMC);						for(var i=0 ; i <nfrascos; i++) {				var f = new CFrasco();				f.x = stage.stageWidth  / 2;				f.y = stage.stageHeight / 2;				frascosMC.addChild(f);				//frascos.push(f);			}					  fazGemada();				}				private function fazGemada() {			for(var i=0 ; i <ngemas; i++) {				var o = new COvo(frascosMC);				o.x = stage.stageWidth  / 2;				o.y = stage.stageHeight / 2;				addChild(o);			}		}	}}